class Solution {
    public int missingNumber(int[] nums) {
       
       boolean[] arr = new boolean[nums.length+1];

       for(int i : nums){
        arr[i] = true;
       }

       for(int i = 0; i <= nums.length; i++){
        if(arr[i] == false){
            return i;
        }
       }
       return -1;
  }
}
  
